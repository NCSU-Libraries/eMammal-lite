<%= form_for(@identification, html: { class: "flex-column-center choice-form" }, remote: true) do |f| %>
  <%= hidden_field_tag "identification[photo_id]", @photo.id%>
  <% if user_signed_in? %>
    <%= hidden_field_tag "identification[user_id]", current_user.id %>
  <% else %>
    <%= hidden_field_tag "identification[user_id]", 9999 %>
  <% end %>
  <% @animals.each do |el| %>
    <%=
      f.button el[:name],
      value: el[:id],
      name: "identification[user_identification]",
      class: "emammal-btn dark-btn choice-btn lg-text"
    %>
  <% end %>
<% end %>
